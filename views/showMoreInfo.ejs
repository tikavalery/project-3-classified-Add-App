      <img class="d-block w-100" src="<%= oneitem.picture1%>" alt="First slide" height="400" width="500">
    </div>
    <div class="carousel-item">
      <img class="d-block w-100" src="<%= oneitem.picture2%>" alt="Second slide" height="400" width="500">
    </div>
    <div class="carousel-item">
      <img class="d-block w-100" src="<%= oneitem.picture3%>" alt="Third slide" height="400" width="500">
    </div>
    <div class="carousel-item">
      <img class="d-block w-100" src="<%= oneitem.picture4%>" alt="Third slide" height="400" width="500">
